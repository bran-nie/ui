{
    "name": "makex-ui",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite --config ./build/base.ts",
        "build": "vue-tsc --noEmit && vite build",
        "build:docs": "vue-tsc --noEmit && vite build --config ./build/docs.config.ts && node script/copyDir.js",
        "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
        "preview": "vite preview",
        "gen": "node ./script/genNewComp/index.js"
    },
    "dependencies": {
        "prismjs": "^1.25.0",
        "vue": "^3.2.25"
    },
    "devDependencies": {
        "@types/node": "^17.0.18",
        "@types/prismjs": "^1.16.6",
        "@vitejs/plugin-vue": "^2.0.0",
        "@vitejs/plugin-vue-jsx": "^1.3.7",
        "fs-extra": "^10.0.0",
        "handlebars": "^4.7.7",
        "inquirer": "^8.2.0",
        "less": "^4.1.2",
        "typescript": "^4.4.4",
        "vite": "^2.7.2",
        "vite-plugin-dts": "^0.9.6",
        "vite-plugin-md": "^0.11.7",
        "vue-router": "4",
        "vue-tsc": "^0.29.8"
    }
}
